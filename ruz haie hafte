	if(a==1)
	    cout<<a<<" = shanbe";
    else if(a==2)
        cout<<a<<" =1shanbe";
    else if(a==3)
        cout<<a<<" =2shanbe";
    else if(a==4)
        cout<<a<<" =3shanbe";
    else if(a==5)
        cout<<a<<" =4shanbe";
    else if(a==6)
        cout<<a<<" =5shanbe";
    else if(a==7)
